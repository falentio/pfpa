{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "pfpa-web",
    "pages_build_output_dir": "dist",
    "compatibility_date": "2025-04-01",
    "compatibility_flags": ["nodejs_compat"],
    "r2_buckets": [
        {
            "bucket_name": "pfpa-bucket",
            "binding": "R2"
        }
    ],
    "kv_namespaces": [
        {
            "binding": "KV",
            "id": "8d2c184f15d2475ba75d715d969769fd"
        }
    ],
    "ai": {
        "binding": "AI"
    },
    "observability": {
        "enabled": true
    }
}
